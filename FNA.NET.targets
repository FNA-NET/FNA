<Project>
    <ItemGroup>
        <Files Include="$(MSBuildThisFileDirectory)/../contentFiles/*.*" />
    </ItemGroup>
    <Target Name="CopyFiles" AfterTargets="Build" Condition="'$(TargetFramework)' != 'net9.0-android' And '$(TargetFramework)' != 'net9.0-ios' And '$(TargetFramework)' != 'net9.0-tvos'">
        <Copy SourceFiles="@(Files)" DestinationFolder="$(TargetDir)" />
    </Target>
</Project>
